TRUSTEDCORE_LOCAL_CFLAGS := \
				-DTEXT_BASE=$(TRUSTEDCORE_TEXT_BASE) \
			    -DTASK_BASE=$(TRUSTEDCORE_TASK_BASE) \
			    -DTRUSTEDCORE_MEM_SIZE=$(TRUSTEDCORE_MEM_SIZE) \
			    -DTRUSTEDCORE_KERNEL_MEM_SIZE=$(TRUSTEDCORE_KERNEL_MEM_SIZE) \
			    -DTRUSTEDCORE_TA_SHARE_MEM_SIZE=$(TRUSTEDCORE_TA_SHARE_MEM_SIZE) \
			    -DTRUSTEDCORE_TA_RUN_MEM_SIZE=$(TRUSTEDCORE_TA_RUN_MEM_SIZE) \
			    -DTASK_MEM_BASE_ADDRESS=$(TRUSTEDCORE_TASK_MEM_BASE) \
			    -DTASK_MEM_LOAD_ADDRESS=$(TRUSTEDCORE_TASK_LOAD_BASE) \
			    -DTRUSTEDCORE_VIRT_BASE=$(TRUSTEDCORE_VIRT_BASE) \
			    -DTRUSTEDCORE_RELEASE_VERSION=$(TRUSTEDCORE_VERSION) 


ifeq ($(strip $(WITH_DEBUG_VERSION)), true)
TRUSTEDCORE_LOCAL_CFLAGS += -DDEBUG_VERSION=$(WITH_DEBUG_VERSION)
endif
ifeq ($(SECURE_TESTSUITE), RTOSck_UT)
TRUSTEDCORE_LOCAL_CFLAGS += -DTESTSUITE_RTOSck_UT
ifeq ($(strip $(TRUSTEDCORE_ASFLAG_SET)), true)
TRUSTEDCORE_LOCAL_ASFLAGS += -DTESTSUITE_RTOSck_UT
endif
WITH_DEBUG_SWITCH :=true
endif
ifeq ($(SECURE_TESTSUITE), RTOSck_PT)
TRUSTEDCORE_LOCAL_CFLAGS += -DTESTSUITE_RTOSck_PT
ifeq ($(strip $(TRUSTEDCORE_ASFLAG_SET)), true)
TRUSTEDCORE_LOCAL_ASFLAGS += -DTESTSUITE_RTOSck_PT
endif
WITH_DEBUG_SWITCH :=true
endif
ifeq ($(SECURE_TESTSUITE), RTOSck_IT)
TRUSTEDCORE_LOCAL_CFLAGS += -DTESTSUITE_RTOSck_IT
ifeq ($(strip $(TRUSTEDCORE_ASFLAG_SET)), true)
TRUSTEDCORE_LOCAL_ASFLAGS += -DTESTSUITE_RTOSck_IT
endif
WITH_DEBUG_SWITCH :=true
endif

ifeq ($(strip $(LPAE_SUPPORT)), 1)
    TRUSTEDCORE_LOCAL_CFLAGS += -DARM_PAE
    TRUSTEDCORE_LOCAL_ASFLAGS += -DARM_PAE
endif

ifeq ($(strip $(WITH_MEDIAOS_BRANCH)), true)
TRUSTEDCORE_LOCAL_CFLAGS += -DWITH_MEDIAOS_BRANCH
endif

#MUST IN THE END OF THIS FILE
ifeq ($(strip $(TRUSTEDCORE_ASFLAG_SET)), true)
TRUSTEDCORE_LOCAL_ASFLAGS := $(TRUSTEDCORE_LOCAL_CFLAGS)
endif
