<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item android:id="@android:id/background">
<shape>
<corners android:radius="5dip" />
<gradient
android:startColor="#fffffafa"
android:centerColor="#ff111111"
android:centerY="0.75"
android:endColor="#ffcdc9c9"
android:angle="270"
/>
</shape>
</item>


<item android:id="@android:id/secondaryProgress">
<clip>
<shape>
<corners android:radius="5dip" />
<gradient
android:startColor="#80bfefff"
android:centerColor="#80b2dfee"
android:centerY="0.75"
android:endColor="#a09ac0cd"
android:angle="270"
/>
</shape>
</clip>
</item>


<item android:id="@android:id/progress">
<clip>
<shape>
<corners android:radius="5dip" />
<gradient
android:startColor="#ffffffff"
android:centerColor="#ff3971ff"
android:centerY="0.75"
android:endColor="#ff1212ff"
android:angle="270"
/>
</shape>
</clip>
</item>

</layer-list>
